{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      // "@/*": ["./*"],
      "@assets/*": ["assets/*"],
      "@config": ["config.ts"],
      "@components/*": ["components/*"],
      "@content/*": ["content/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"]
    }
  }
  // "include": ["src", "node_modules/astro-i18next/dist/types/types.d.ts"]
}
